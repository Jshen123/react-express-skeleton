{
  "name": "react-express",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@babel/core": "latest",
    "@babel/node": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@material-ui/core": "latest",
    "babel-loader": "latest",
    "body-parser": "^1.19.0",
    "cross-env": "latest",
    "dotenv": "^8.2.0",
    "express": "latest",
    "knex": "^0.20.7",
    "nodemon": "latest",
    "npm-run-all": "latest",
    "react": "latest",
    "react-dom": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "scripts": {
    "start": "webpack -w & nodemon --exec yarn babel-node -- src/server.js",
